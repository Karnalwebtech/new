{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "dist",
    "lib": ["es2019"],
    "target": "ES2018",
    "isolatedModules": false,
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["server.ts"]
}
